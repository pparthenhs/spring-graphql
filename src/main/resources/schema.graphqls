type Owner {
    id: ID!
    name: String!
}

type Vehicle {
    id: ID!
    label: String!
    mark: String
    owner: Owner!
}

type Query {
    allOwners: [Owner]
    allOwnersLastTwo (last: Int): [Owner]
    allOwnersContainsName (name: String): [Owner]
    allVehicles: [Vehicle]
    hasVehicles (id: String): Owner
}

type Mutation {
   createOwner (name: String): Owner
}

type Subscription {
   allOwnersSub: [Owner]
}